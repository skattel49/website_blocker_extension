{
    "name": "Chrome Extension Example",
    "version": "1.0",
    "description": "Learning about Chrome Extensions",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
     },
     "browser_action": {
        "default_popup": "popup.html"
     },
     "options_page": "options.html",
     "permissions": [
         "tabs"
     ]
}
